{
    "private": true,
    "name": "boilerplate-webpack-babel",
    "version": "1.2.0",
    "description": "Starter with Webpack & Babel.js",
    "license": "MIT",
    "author": {
        "name": "Piotr Kowalski",
        "email": "piecioshka@gmail.com",
        "url": "https://piecioshka.pl"
    },
    "scripts": {
        "test": "jest",
        "clear:dist": "del /q/s/f dist && rmdir dist",
        "clear:NM": "del /q/s/f node_modules && rmdir node_modules",
        "clear:all": "npm run clear:dist && npm run clear:NM",
        "prebuild": "npm run clear",
        "build": "npm run build:development",
        "build:development": "webpack --config webpack/webpack.config.js",
        "prod": "webpack --config webpack/webpack.config.js --env.production",
        "watch": "npm run build:development -- -w",
        "start": "node src/server/server.js",
        "dev": "webpack-dev-server --config webpack/webpack.config.js --open"
    },
    "devDependencies": {
        "@babel/cli": "^7.10.5",
        "@babel/core": "^7.10.2",
        "@babel/node": "^7.10.5",
        "@babel/polyfill": "^7.10.4",
        "@babel/preset-env": "^7.10.2",
        "babel-loader": "^8.1.0",
        "babel-plugin-transform-runtime": "^6.23.0",
        "clean-webpack-plugin": "^3.0.0",
        "css-loader": "^3.6.0",
        "file-loader": "^6.0.0",
        "html-loader": "^1.1.0",
        "html-webpack-plugin": "^4.3.0",
        "http-server": "^0.12.3",
        "jest": "^26.1.0",
        "mini-css-extract-plugin": "^0.9.0",
        "optimize-css-assets-webpack-plugin": "^5.0.3",
        "sass": "^1.26.10",
        "sass-loader": "^9.0.2",
        "style-loader": "^1.2.1",
        "terser-webpack-plugin": "^3.0.7",
        "url-loader": "^4.1.0",
        "webpack": "^4.43.0",
        "webpack-bundle-analyzer": "^3.8.0",
        "webpack-cli": "^3.3.11",
        "webpack-dev-server": "^3.11.0",
        "webpack-merge": "^4.2.2"
    },
    "dependencies": {
        "body-parser": "^1.19.0",
        "cors": "^2.8.5",
        "express": "^4.17.1",
        "node-fetch": "^2.6.0"
    }
}
